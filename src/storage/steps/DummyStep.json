        },
        "default" : "BertForNounClassification",
        "description" : "Model used for Classification"
      },
      "tokenization" : "bool",
      "num heads": {
        "type" : "int",
        "input" : {
          "type" : "slider",
          "min" : 4,
          "max" : 12,
          "step" : 2
        }
      }
    },
    "dimensions" : {
      "type" : "bool",
      "default": true
    }
  },
  "inputs": {
    "text" : "string",
    "original text" : "string"
  },
  "outputs" : {
    "detected nouns" : "list[int]",
    "visualization" : "html"
